package tema52;

public class ConductorHora {

	public static void main(String[] args) {
		Hora rolex = new Hora();
		rolex.ImpHora();
		rolex.LeerHora(12,42,02);
		rolex.ImpHora();
		rolex.SumHora(12);
		rolex.ImpHora();
		rolex.SumMin(0);
		rolex.ImpHora();
		rolex.SumSeg(0);
		rolex.ImpHora();
	}

}
