package tema52;

public class Conductor {

	public static void main(String[] args) {
		Trabajador tra = new Trabajador();
		Trabajador tra1 = new Trabajador();
		tra.NuevoTrabajador();
		tra.VerTrabajador();
		tra.ModTrabajador();
		tra.VerTrabajador();
		tra.Salario();
		tra1.NuevoTrabajador();
		tra1.VerTrabajador();
		tra1.ModTrabajador();
		tra1.VerTrabajador();
		tra1.Salario();
	}

}
